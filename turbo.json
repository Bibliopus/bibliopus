{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".output/**",
        "dist/**",
        ".nuxt/**",
        ".vercel/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "generate": {
      "outputs": [
        ".output/**",
        "dist/**",
        ".nuxt/**",
        ".vercel/**",
        "android"
      ]
    }
  }
}