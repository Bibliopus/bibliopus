{
  "name": "bibliopus",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/mobile/*",
    "apps/web/*",
    "apps/api/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "app:generate": "turbo run generate --filter=@bibliopus/shelf"
  },
  "devDependencies": {
    "@bibliopus/eslint-config": "*",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.19.3"
}
